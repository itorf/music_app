<h1><%= @track.name%></h1>


<%= ugly_lyrics(@track.lyrics) if @track.lyrics %>



<%= button_to "#{@track.album.name}", album_url(@track.album), method: :get %>
<%= button_to "Edit Track", edit_track_url(@track), method: :get %>
<%= button_to "Delete Track", track_url(@track), method: :delete %>

<%= render "notes/form", track: @track %>
</br>
<%= render "notes/notes", track: @track %>